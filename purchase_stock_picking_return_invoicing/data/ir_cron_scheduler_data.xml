<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<data noupdate="1">
	    <!-- Cron Reprocesamiento de las lineas de compra faltantes -->
   	    <record id="process_pending_action_compute_purchase_line_qty" model="ir.cron">
	        <field name="name">Procesar todas las lineas de compra de todas las compras, marcando las que ya fueron calculadas</field>
	        <field name="user_id" ref="base.user_root"/>
	        <field name="interval_number">10</field>
	        <field name="interval_type">minutes</field>
	        <field name="numbercall">-1</field>
	        <field name="doall" eval="False"/>
	        <field name="model" eval="'purchase.order'"/>
	        <field name="function" eval="'cron_compute_purchase_line_qty'"/>
	        <field name="args" eval="'()'"/>
	    </record>
	    
	</data>
</odoo>